{
  "configurations": [
    {
      "name": "ESP-IDF",
      "compilerPath": "${config:idf.toolsPath}/tools/xtensa-esp-elf/esp-14.2.0_20241119/xtensa-esp-elf/bin/xtensa-esp32-elf-gcc",
      "compileCommands": "${workspaceFolder}/examples/connect_and_print_info/build/compile_commands.json",
      "includePath": [
        "${workspaceFolder}/examples/connect_and_print_info/build/config",
        "${workspaceFolder}/include/**",
        "${config:idf.espIdfPath}/components/**"
      ],
      "browse": {
        "path": [
          "${config:idf.espIdfPath}/components",
          "${workspaceFolder}"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ],
  "version": 4
}
